<h1>Sign In</h1>
<%= form_for :session do |s| %>
	<%= s.text_field :email, placeholder: "E-mail" %>
	<%= s.password_field :password, placeholder: "Password" %>
	<%= s.password_field :password_confirmation, placeholder: "Password Confirmation" %>
	<%= s.submit "Log In" %>
<% end %>
<%= link_to "Back", root_path %>